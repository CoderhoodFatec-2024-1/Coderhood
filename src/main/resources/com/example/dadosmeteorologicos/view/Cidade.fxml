<AnchorPane prefHeight="600" prefWidth="900" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dadosmeteorologicos.controller.CidadeController">
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
   <children>
      <TableView fx:id="tabelaCidades" layoutX="292.0" layoutY="81.0" prefHeight="287.0" prefWidth="317.0">
        <columns>
          <TableColumn fx:id="colunaNome" prefWidth="117.0" text="Nome" />
          <TableColumn fx:id="colunaSigla" prefWidth="75.0" text="Sigla" />
          <TableColumn fx:id="colunaBotao" prefWidth="122.0" />
        </columns>
      </TableView>
      <Button fx:id="btnCriar" layoutX="397.0" layoutY="397.0" mnemonicParsing="false" onAction="#criarCidade" text="Adicionar Cidade" />
   </children>
</AnchorPane>
